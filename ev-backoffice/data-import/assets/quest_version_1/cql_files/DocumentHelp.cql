CREATE (DocumentHelp_dochelp_113:DocumentHelp{easyVisaId:"DocHelp_113",questVersion:"quest_version_1",name:"U.S. Citizenship Evidence #601A",tooltip:"<div id='DocHelp_113'>
            <h4 style='text-decoration: underline;text-align: center'>I-601A: Application for Provisional Unlawful
                Presence Waiver </h4>
            If you claim that an 'extreme hardship' will result to your U.S. citizen spouse or parent, but this is not
            the
            same person who petitioned for your immigrant visa, you must present evidence of this person's U.S.
            citizenship.
            <ol style='list-style-type: none'>
                <li>
                    Evidence of U.S. citizenship includes, but is not limited to, any of the following:
                    <ul style='list-style-type:decimal'>
                        <li>If your relative was born in the United States, a copy of his or her birth certificate,
                            issued
                            by a civil registrar, vital statistics office, or other civil authority of a U.S. state,
                            county,
                            municipal authority, or territory; or</li>
                        <li>A copy of your relative's naturalization certificate or certificate of citizenship issued by
                            USCIS or the former Immigration and Naturalization Service (INS); or</li>
                        <li>A copy of your relative's unexpired U.S. passport; or </li>
                        <li>A copy of your relative's DOS-issued Form FS-240, Report of Birth Abroad of a Citizen of the
                            United States. </li>
                    </ul>
                </li>
            </ol>
        </div>",order:113});
CREATE (DocumentHelp_dochelp_114:DocumentHelp{easyVisaId:"DocHelp_114",questVersion:"quest_version_1",name:"U.S. Citizenship Evidence #864",tooltip:"  <div id='DocHelp_114'>
