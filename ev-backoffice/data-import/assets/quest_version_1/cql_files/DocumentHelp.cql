CREATE (DocumentHelp_dochelp_98:DocumentHelp{easyVisaId:"DocHelp_98",questVersion:"quest_version_1",name:"Photographs #751",tooltip:"  <div id='DocHelp_98'>
            <h4 style='text-decoration: underline;text-align: center'>I-751: Petition to Remove Conditions on Residence
